<script nonce="{{ Metadata.JWN }}">
function copyToClipboard(ID) {
    var element = document.getElementById(`${ID}`);
    let data = element.textContent;
    element.style.color = "gray";
    console.log(`Clipboard Event ⭌ ${ID}: (${data})`);
    $("body").toast({
        title: `${data}`,
        message: "<strong style='margin-left: 0.435em;margin-right:0.435em;font-size:1.25em;'>↳</strong><strong>Clipboard</strong>",
        displayTime: 4250,
        showProgress: "bottom",
        progressUp: true,
        classProgress: "blue",
        compact: false,
        showIcon: "copy outline",
        opacity: 0.785,
        class: "inverted",
        closeIcon: false,
        showMethod   : "zoom",
        hideMethod   : "fade",
    });
    setTimeout(() => { element.style.color = "white"; }, 4250);
    if (window.clipboardData && window.clipboardData.setData) {
        return clipboardData.setData("Text", data);
    } else if (document.queryCommandSupported && document.queryCommandSupported("copy")) {
        var textarea = document.createElement("textarea");
        textarea.textContent = data;
        textarea.style.position = "fixed";
        document.body.appendChild(textarea);
        textarea.select();
        try {
            return document.execCommand("copy");
        } catch (ex) {
            console.warn("Copy to clipboard failed.", ex);
            return false;
        } finally {
            document.body.removeChild(textarea);
        }
    }
};

async function modifyVPSTable() {
    console.log("VPS-Table-Modification Event");
    
    const VPSDecommissionButton = document.getElementById("VPS-Decommission-Button");
    const VPSInitializeButton = document.getElementById("VPS-Initialize-Button");
    const VPSProvisionButton = document.getElementById("VPS-Provision-Button");

    if ($("tr.vps.row").children("td.vps").hasClass("selectable")) {
        $("tr.vps.row").children("td.vps").removeClass("selectable");
        $("tr.vps.row").children("td.vps").children("a").css("color", "gray");
        
        VPSDecommissionButton.classList.remove("disabled");
        VPSInitializeButton.classList.remove("disabled");
        VPSProvisionButton.classList.remove("disabled");
    } else {
        try {
            $("tr.vps.row").children("td.vps").addClass("selectable");
            $("tr.vps.row").children("td.vps").children("a").css("color", "unset");
        } catch {
            $("tr.vps.row").children("td.vps").addClass("selectable");
            $("tr.vps.row").children("td.vps").children("a").css("color", "unset");
        } finally {
            if (VPSDecommissionButton.classList.hasClass("disabled")) {
                VPSDecommissionButton.classList.remove("disabled");
            } else {
                VPSDecommissionButton.classList.add("disabled");
            }
            if (VPSInitializeButton.classList.hasClass("disabled")) {
                VPSInitializeButton.classList.remove("disabled");
            } else {
                VPSInitializeButton.classList.add("disabled")
            }
            if (VPSProvisionButton.classList.hasClass("disabled")) {
                VPSProvisionButton.classList.remove("disabled");
            } else {
                VPSProvisionButton.classList.add("disabled");
            }
            return true
        }
    }
};

document.getElementById("VPS-Modifier-Button").addEventListener("click", function() {
    this.classList.add("loading");
    modifyVPSTable().then(
        setTimeout(() => {
            this.classList.remove("loading");
            this.textContent = "Lock";
        }, 1000));
});

async function modifyServerTable() {
    console.log("Server-Table-Modification Event");
    if ($("tr.server.row").children("td.server").hasClass("selectable")) {
        $("tr.server.row").children("td.server").removeClass("selectable");
        $("tr.server.row").children("td.server").children("a").css("color", "gray");
    } else {
        try {
            $("tr.server.row").children("td.server").addClass("selectable");
            $("tr.server.row").children("td.server").children("a").css("color", "unset");
        } catch {
            $("tr.server.row").children("td.server").addClass("selectable");
            $("tr.server.row").children("td.server").children("a").css("color", "unset");
         } finally { return true; }
    }
};

document.getElementById("Server-Modifier-Button").addEventListener("click", function() {
    this.classList.add("loading");
    modifyServerTable().then(
        setTimeout(() => {
            this.classList.remove("loading");
            this.textContent = "Lock";
        }, 1000));
});

