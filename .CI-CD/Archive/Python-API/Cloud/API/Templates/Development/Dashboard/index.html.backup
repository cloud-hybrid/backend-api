{% extends "Base.html" %}

{% block head %}
{{ super() }}

<!-- {{ Metadata.Title }} CSS -->
<link nonce="{{ Metadata.JWN }}" type="text/css" rel="stylesheet" href="{{ url_for('static', filename='./Cloud-Theme/Styles/_Dashboard.css') }}">
<link nonce="{{ Metadata.JWN }}" type="text/css" rel="stylesheet" href="{{ url_for('static', filename='./Cloud-Theme/Styles/AWS-EC2-Provision-Form.css') }}">

<style>
a.disabled {
    pointer-events: none;
    cursor: default;
}
.ui.table tbody tr td.selectable > a:not(.ui) {
    text-align: center;
    overflow-x: overlay;
}
.selectable { cursor: pointer; }

.ui.tooltip.button {
    z-index: 1;
}

span.under-development.tool-tip::after {
    font-style: italic;
    font-weight: bold;
}

main {
    min-height: 100vh;
    padding-top: 1.0rem;
    background: rgb(235, 235, 235)
}

main > section:first-child {
    margin-top: calc(63px + 0.25rem);
}
.ui.inverted.table > tbody > tr > td, .ui.inverted.table > tfoot > tr > td, .ui.inverted.table > tr > td {
    overflow-x: overlay;
}

main.ui.fluid.padded.container {
    margin-left: auto;
    margin-right: auto;
    padding-left: 4.0rem;
    padding-right: 4.0rem;
}

/* .ui.toast:not(.vertical) > .icon:not(.close) + .content {
    padding-left: 2.425rem;
}
.ui.toast:not(.vertical) > .icon:not(.close) + .content i {
    padding-left: 3px;
}
.ui.toast-container .toast-box .ui.toast:not(.vertical) {
    display: block;
    min-width: 150px;
}
.inverted.ui.toast {
    color: rgba(255, 255, 255, 1.0)
} */
/* .inverted.ui.toast.visible {
    padding-right: 1.625rem;
} */
/* .inverted.ui.toast.visible .content .ui.header { font-size: 16px; }

.ui.toast-container.top.right {
    top: 4.91875rem;
    right: 0.875rem;
    margin-left: 0.85em;
}
.ui.toast:not(.vertical) > .centered.image, .ui.toast:not(.vertical) > .centered.icon {
    font-size: 25px;
    padding-right: 0.385rem;
    padding-top: 0.25rem;
} */

.ui.toast-container.top.right {
    top: calc(63px + 0.25em);
    right: 0.875rem;
    margin-left: 0.85em;
}

.ui.toast:not(.vertical) > .centered.image, .ui.toast:not(.vertical) > .centered.icon {
    margin: auto;
}

.ui.toast > .icon:not(.close) {
    font-size: 1.85em;
}
</style>

{% endblock %}

{% block sidemenu %}
{% include "./Side-Menu.html" %}
{% endblock %}

{% block menu %}
{% include "./Primary-Menu.html" %}
{% endblock %}

{% block pusher %}

<main class="ui fluid padded container">
    <section name="Header" class = "ui left aligned row">
        <h1 class="ui big inline header">
            Management Console
        </h1>
        <button class="circular ui icon large disabled button"><i class="red exclamation circle icon"></i></button>
        <button class="circular ui icon large disabled button"><i class="yellow exclamation triangle icon"></i></button>
        <button class="circular ui icon large disabled button"><i class="blue bell icon"></i></button>
    </section>
    <section name = "Overview" class = "ui left aligned two column grid">
        <div class="ten wide column">
            <h3 class="ui header">
                Dashboard
            </h3>
            <div class = "row">
                <div class="ui info fitted message">
                    Ensure to throughly review applicable documentation prior to resource modification(s)
                </div>
                <div id = "API-Endpoints-Accordion" class="io ui styled fluid accordion">
                    <div class="title">
                        <i class="dropdown icon"></i>
                        Overview
                    </div>
                    <div class="content">
                        Terminology, Navigation, & Expectations
                        <div class="accordion transition hidden">
                            <div class="title">
                                Dedicated Hosts <i class="dropdown icon"></i>
                            </div>
                            <div class="content">
                                <p>Level 1A Contents</p>
                                <div class="accordion">
                                    <div class="title">
                                        <i class="dropdown icon"></i>
                                        Level 1A-A
                                    </div>
                                    <div class="content">
                                        Level 1A-A Contents
                                    </div>
                                    <div class="title">
                                        <i class="dropdown icon"></i>
                                        Level 1A-B
                                    </div>
                                    <div class="content">
                                        Level 1A-B Contents
                                    </div>
                                </div>
                            </div>
                            <div class="title">
                                <i class="dropdown icon"></i>
                                Level 1B
                            </div>
                            <div class="content">
                                Level 1B Contents
                            </div>
                            <div class="title">
                                <i class="dropdown icon"></i>
                                Level 1C
                            </div>
                            <div class="content">
                                Level 1C Contents
                            </div>
                        </div>
                    </div>
                    <div class="title">
                        <i class="dropdown icon"></i>
                        Level 2
                    </div>
                    <div class="content">
                        <p class="transition hidden">Welcome to level 2</p>
                        <div class="accordion transition hidden">
                            <div class="active title">
                                <i class="dropdown icon"></i>
                                Level 2A
                            </div>
                            <div class="active content">
                                <p>Level 2A Contents</p>
                                <div class="accordion">
                                    <div class="title">
                                        <i class="dropdown icon"></i>
                                        Level 2A-A
                                    </div>
                                    <div class="content">
                                        Level 2A-A Contents
                                    </div>
                                    <div class="title">
                                        <i class="dropdown icon"></i>
                                        Level 2A-B
                                    </div>
                                    <div class="content">
                                        Level 2A-B Contents
                                    </div>
                                </div>
                            </div>
                            <div class="title">
                                <i class="dropdown icon"></i>
                                Level 2B
                            </div>
                            <div class="content">
                                Level 2B Contents
                            </div>
                            <div class="title">
                                <i class="dropdown icon"></i>
                                Level 2C
                            </div>
                            <div class="content">
                                Level 2C Contents
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <div class = "ui center aligned two column grid">
        <div id="Sticky-Context" class = "ui thirteen wide column">
            <h2 class="ui center aligned big header">
                <strong>Dedicated Hosts</strong>
            </h2>
            <table class="ui selectable celled inverted striped single line fixed table">
                <thead class="full-width">
                    <tr>
                        <th class="ui one wide center aligned small header">Status</th>
                        <th class="ui two wide center aligned small header">Host</th>
                        <th class="ui one wide center aligned small header">⋮</th>
                        <th class="ui one wide center aligned small header">User</th>
                        <th class="ui three wide center aligned small header">IPv4</th>
                        <th class="ui three wide center aligned small header">UUID</th>
                        <th class="ui two wide center aligned small header">LAN</th>
                    </tr>
                </thead>
                <tbody>
                    {% for Host in Hosts %}
                    <tr>
                        <td class="host copy selectable ui center aligned">
                            Online
                        </td>
                        <td class="host copy selectable ui center aligned">
                            <a onclick="javascript:copyToClipboard('{{ Host.Name }}-Field-{{ loop.index }}')">
                                <strong id="{{ Host.Name }}-Field-{{ loop.index }}">{{ Host.Name }}</strong>
                            </a>
                            </div>
                        </td>
                        <td class="selectable ui center aligned">
                            <a href="./dashboard/hosts/{{ Host.UUID }}">
                                <i class="large cogs icon"></i>
                            </a>
                        </td>
                        <td class="host copy selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ Host.Username }}-Field-{{ loop.index }}')">
                                <code id="{{ Host.Username }}-Field-{{ loop.index }}">{{ Host.Username }}</code>
                            </a>
                        </td>
                        <td class="host copy selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ Host.Public }}-Field-{{ loop.index }}')">
                                <code id="{{ Host.Public }}-Field-{{ loop.index }}">{{ Host.Public }}</code>
                            </a>
                        </td>
                        <td class="host copy selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ Host.UUID }}-Field-{{ loop.index }}')">
                                <code id="{{ Host.UUID }}-Field-{{ loop.index }}">{{ Host.UUID }}</code>
                            </a>
                        </td>
                        <td class="host copy selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ Host.Local }}-Field-{{ loop.index }}')">
                                <code id="{{ Host.Local }}-Field-{{ loop.index }}">{{ Host.Local }}</code>
                            </a>
                        </td>
                        <!-- | Controls | -->
                    </tr>
                    {% endfor %}
                </tbody>
                <tfoot class="full-width">
                    <tr>
                        <th colspan="7">
                            <button class="ui link right floated small inverted basic button">
                                <span class="ui inverted white text">Add</span>
                            </button>
                            <button class="ui link right floated small inverted basic button">
                                <span class="ui inverted white text">Add</span>
                            </button>
                            <div class="ui small button">
                                Unlock
                            </div>
                            <span class="under-development tool-tip" data-tooltip="Under Development"
                                data-position="right center" data-variation="small basic" data-inverted="True">
                                <div class="ui small basic inverted icon buttons">
                                    <button class="ui disabled button"><i class="file icon"></i></button>
                                    <button class="ui disabled button"><i class="save icon"></i></button>
                                    <button class="ui disabled button"><i class="upload icon"></i></button>
                                    <button class="ui disabled button"><i class="download icon"></i></button>
                                </div>
                            </span>
                        </th>
                    </tr>
                </tfoot>
            </table>

            <h2 class="ui center aligned big header">
                VPS Instances
            </h2>
            <table class="ui selectable celled inverted striped single line fixed table">
                <thead class="full-width">
                    <tr>
                        <th class="ui one wide center aligned small header">Status</th>
                        <th class="ui three wide center aligned small header">Name</th>
                        <th class="ui one wide center aligned small header">⋮</th>
                        <th class="ui two wide center aligned small header">IPv4</th>
                        <th class="ui one wide center aligned small header">Key</th>
                        <th class="ui one wide center aligned small header">User</th>
                        <th colspan="3" class="ui two wide center aligned small header">Controls</th>
                    </tr>
                </thead>
                <tbody>
                    {% for VPS in Instances %}
                    {% set Name = VPS["Name"] %}
                    {% set IP = VPS["Networking-Public-IP"] %}
                    {% set Username = VPS["SSH-Username"] %}
                    <tr class="vps selectable row">
                        <td id="{{ VPS['UUID'] }}-Status-Indicator" class="selectable ui center aligned">
                            <i class="large i-table center aligned green layer group cell-spinner icon"></i>
                        </td>
                        <td class="vps selectable ui center aligned">
                            <a onclick="javascript:copyToClipboard('{{ Name }}-Field-{{ loop.index }}')">
                                <strong id="{{ Name }}-Field-{{ loop.index }}">{{ Name }}</strong>
                            </a>
                        </td>
                        <td class="vps selectable ui center aligned">
                            <a href="./dashboard/vps/{{VPS.UUID}}">
                                <i class="large i-table center aligned layer cog icon"></i>
                            </a>
                        </td>
                        <td class="vps selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ IP }}-Field-{{ loop.index }}')">
                                <code id="{{ IP }}-Field-{{ loop.index }}">{{ IP }}</code>
                            </a>
                        </td>
                        <td id="{{ VPS['UUID'] }}-SSH-Key" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer download cell-spinner icon"></i>
                        </td>
                        <td class="vps selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ Username }}-Field-{{ loop.index }}')">
                                <strong><code id="{{ Username }}-Field-{{ loop.index }}">{{ Username }}</code></strong>
                            </a>
                        </td>
                        <!-- Controls -->
                        <td id="{{ VPS['UUID'] }}-Power-On" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer green power cell-spinner-green icon"></i>
                        </td>
                        <td id="{{ VPS['UUID'] }}-Power-Off" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer red plug cell-spinner-red icon"></i>
                        </td>
                        <td id="{{ VPS['UUID'] }}-Power-Cycle" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer blue sync alternate cell-spinner-blue icon"></i>
                        </td>
                        <!-- | Controls | -->
                    </tr>
                    {% endfor %}
                </tbody>
                <tfoot>
                    <tr>
                        <th colspan="9">
                                <button id = "VPS-Decommission-Button" class="ui link right floated small inverted basic disabled button">
                                    <span class="ui inverted white text">Decomission</span>
                                </button>
                                <button id = "VPS-Initialize-Button" class="ui link right floated small inverted basic disabled button">
                                    <span class="ui inverted white text">Initialize</span>
                                </button>
                                <button id = "VPS-Provision-Button" class="ui link right floated small inverted basic disabled button">
                                    <span class="ui inverted white text">Provision</span>
                                </button>
                            <div class="ui small button" id="VPS-Modifier-Button">
                                Unlock
                            </div>
        
                            <span class="under-development tool-tip" data-tooltip="Under Development"
                                data-position="right center" data-variation="small basic" data-inverted="True">
                                <div class="ui small basic inverted icon buttons">
                                    <button class="ui disabled button"><i class="file icon"></i></button>
                                    <button class="ui disabled button"><i class="save icon"></i></button>
                                    <button class="ui disabled button"><i class="upload icon"></i></button>
                                    <button class="ui disabled button"><i class="download icon"></i></button>
                                </div>
                            </span>
                        </th>
                    </tr>
                </tfoot>
            </table>
            <h2 class="ui center aligned big header">
                Server Daemons
            </h2>
            <table class="ui selectable celled inverted striped single line fixed table">
                <thead class="full-width">
                    <tr>
                        <th class="ui center aligned small one wide header">Status</th>
                        <th class="ui four wide center aligned small header">Name</th>
                        <th class="ui center aligned small one wide header">⋮</th>
                        <th class="ui center aligned small header">VPS</th>
                        <th colspan="3" class="ui center aligned small header">Controls</th>
                    </tr>
                </thead>
                <tbody>
                    {% for Server in Servers %}
                    {% set Name = Server["Name"] %}
                    {% set Instance = Server["VPS-Name"] %}
                    <tr class="server selectable row">
                        <td id="{{ Server['UUID'] }}-Status-Indicator" class="selectable ui center aligned">
                            <i class="large i-table center aligned green layer group cell-spinner icon"></i>
                        </td>
                        <td class="server selectable ui center aligned">
                            <a onclick="javascript:copyToClipboard('{{ Name }}-Field-{{ loop.index }}')">
                                <strong id="{{ Name }}-Field-{{ loop.index }}">{{ Name }}</strong>
                            </a>
                        </td>
                        <td class="server selectable ui center aligned">
                            <a href="./dashboard/server/{{ Server.UUID }}">
                                <i class="large i-table center aligned layer cog icon"></i>
                            </a>
                        </td>
                        <td class="server selectable ui center aligned collapsing">
                            <a onclick="javascript:copyToClipboard('{{ Instance }}-Field-{{ loop.index }}')">
                                <code id="{{ Instance }}-Field-{{ loop.index }}">{{ Instance }}</code>
                            </a>
                        </td>
                        <!-- Controls -->
                        <td id="{{ Server['UUID'] }}-Power-On" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer green power cell-spinner-green icon"></i>
                        </td>
                        <td id="{{ Server['UUID'] }}-Power-Off" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer red plug cell-spinner-red icon"></i>
                        </td>
                        <td id="{{ Server['UUID'] }}-Power-Cycle" class="selectable ui center aligned">
                            <i class="large i-table center aligned layer blue sync alternate cell-spinner-blue icon"></i>
                        </td>
                        <!-- | Controls | -->
                    </tr>
                    {% endfor %}
                </tbody>
                <tfoot>
                    <tr>
                        <th colspan="7">
                            <a href="../API/Database?Database=Cloud-API&Table=Server&Operation=Create">
                                <button class="ui link right floated small inverted basic button">
                                    <span class="ui inverted white text">Create</span>
                                </button>
                            </a>
                            <div class="ui small button" id="Server-Modifier-Button">
                                Unlock
                            </div>
        
                            <span class="under-development tool-tip" data-tooltip="Under Development"
                                data-position="right center" data-variation="small basic" data-inverted="True">
                                <div class="ui small basic inverted icon buttons">
                                    <button class="ui disabled button"><i class="file icon"></i></button>
                                    <button class="ui disabled button"><i class="save icon"></i></button>
                                    <button class="ui disabled button"><i class="upload icon"></i></button>
                                    <button class="ui disabled button"><i class="download icon"></i></button>
                                </div>
                            </span>
                        </th>
                    </tr>
                </tfoot>
            </table>
        </div>
        <div class="ui two wide center aligned column">
            <div id="Sticky-References" class="ui sticky">
                <h3 class="ui header">
                    Stuck Content
                </h3>
                <details>
                    <summary>Sectional Title</summary>
                    <ul>
                        <li>List Item</li>
                        <li>List Item</li>
                        <li>List Item</li>
                        <li>List Item</li>
                        <li>List Item</li>
                        <li>List Item</li>
                    </ul>
                </details>
            </div>
        </div>
    </div>
</main>

<!--
==============================
    Server-Provision Modal
==============================
-->

<div class="ui dimmer modals page transition hidden">
    <div id="Modal-Provision-Server" class="ui inverted transition modal hidden">
        <div class="ui icon header">
            <i class="exclamation circle yellow inverted icon"></i>
        </div>
        <div class="scrolling inverted content">
            <div class="ui very padded vertical raised segment">
                <div class="ui middle aligned stackable grid">
                    <div class="very padded centered row">
                        <div class="fourteen wide left aligned column">
                            <div class="ui inverted raised segment form">
                                <a href="https://aws.amazon.com/ec2/features/?trk=ec2_landing" rel="noreferrer noopener"
                                    target="_blank" class="ui blue ribbon label">VPS</a>
                                <span class="ui header"><strong>Provisioning Form</strong></span>
                                <hr>
                                <form class="ui inverted form" method="POST" autocomplete="off">
                                    <div class="inverted field">
                                        <label>Access ID<button id="AWS-Access-ID-Information"
                                                Data-Link="https://docs.aws.amazon.com/general/latest/gr/aws-sec-cred-types.html#access-keys-and-secret-access-keys"
                                                Data-Tooltip = "An API Token-ID (Click for more details)"
                                                Data-Variation="small basic"
                                                Data-Position="right center"
                                                Data-Inverted = "True"
                                                class="ui tertiary small inverted icon button">
                                                <i class="inverted field question circle outline icon"></i>
                                            </button>
                                        </label>
                                        <input required type="" name="AWS-Access-ID" value=""
                                            placeholder="YVAJOGJ1UGGGCSYSRM3DXY2BS6K" data-validate="Data-AWS-Access-ID">
                                    </div>
                                    <div class="inverted field">
                                        <label>Access Token<button id="AWS-Access-Key-Information"
                                                Data-Link="https://docs.aws.amazon.com/general/latest/gr/aws-sec-cred-types.html#access-keys-and-secret-access-keys"
                                                Data-Tooltip = "A Uniqute & Private Token (Click for more details)"
                                                Data-Variation="small basic"
                                                Data-Position="right center"
                                                Data-Inverted = "True"
                                                class="ui tertiary small inverted icon button">
                                                <i class="inverted field question circle outline icon"></i>
                                            </button>
                                        </label>
                                        <input required type="password" name="SSH-Username" value=""
                                            placeholder="********************" data-validate="Data-AWS-Access-ID">
                                    </div>
                                    <div class="fields">
                                        <div class="inverted five wide field">
                                            <label>SSH Username<button id="AWS-SSH-Username-Information"
                                                style = "color: lightgray;font-weight: bold;"
                                                    Data-Link="https://www.ssh.com/ssh/key/"
                                                    Data-Tooltip = "Read-Only"
                                                    Data-Variation="small basic"
                                                    Data-Position="bottom right"
                                                    Data-Inverted = "True"
                                                    class="ui tertiary small inverted icon button">
                                                    <i class="inverted field question circle outline icon"></i>
                                                </button>
                                            </label>
                                            <input required type="text" name="SSH-Username" value="ubuntu" readonly
                                                placeholder="VPS-Administrator" data-validate="Data-SSH-Username">
                                        </div>
                                        <div class="inverted eleven wide field">
                                            <label>Public Access Key<button id="AWS-SSH-Public-Key-Information"
                                                    Data-Link="https://www.ssh.com/cryptography/public-key"
                                                    class="ui tertiary small inverted icon button">
                                                    <i class="inverted field question circle outline icon"></i>
                                                </button><button id="AWS-SSH-Public-Key-Generator"
                                                    Data-Tooltip="Auto-Generate SSH Key-Pair"
                                                    Data-Position="right center"
                                                    Data-Variation="small basic"
                                                    Data-Inverted="True"
                                                    class="ui tertiary small inverted icon button">
                                                    <i class="inverted field shield icon"></i>
                                                </button>
                                            </label>
                                            <input required type="text" name="SSH-Public-Key" value=""
                                                placeholder="ssh-rsa AAAAB3NC [...]" data-validate="Data-SSH-Public-Key">
                                        </div>
                                    </div>
                                    <div class="inverted field">
                                        <label>Private Access Key <button id="AWS-Private-SSH-Key-Information"
                                                Data-Link="https://www.ssh.com/ssh/key/"
                                                class="ui tertiary small inverted icon button">
                                                <i class="inverted field question circle outline icon"></i>
                                            </button>
                                        </label>
                                        <textarea required class="pre-wrap-field" type="password" name="SSH-Private-Key" value=""
                                            placeholder="ssh-rsa [...]" data-validate="Data-SSH-Private-Key"></textarea>
                                    </div>
                                    <div class="field">
                                        <label>VPS UUID <button id="UUID-Information"
                                                Data-Link="https://en.wikipedia.org/wiki/Universally_unique_identifier"
                                                class="ui tertiary small inverted icon button"
                                                Data-Tooltip="General Overview & Usage" Data-Position="right center"
                                                Data-Variation="small basic" Data-Inverted="True">
                                                <i class="inverted field question circle outline icon"></i>
                                            </button>
                                        </label>
                                        <div class="ui action input">
                                            <input type="text" name="VPS-UUID" placeholder="a490695b-d07b-498e-baa2-f62b7254ad67"
                                                Data-Validate="Data-VPS-UUID" value="">
                                            <div class="UUID-Control-Buttons ui icon inverted buttons">
                                                <button class="UUID-Control-Button ui button" Data-Tooltip="Generate"
                                                    Data-Inverted="True" Data-Variation="small basic">
                                                    <i class="cogs icon"></i>
                                                </button>
                                                <button class="UUID-Control-Button ui button" Data-Tooltip="Generate"
                                                    Data-Inverted="True" Data-Variation="small basic">
                                                    <i class="copy icon"></i>
                                                </button>
                                                <button class="UUID-Control-Button ui button" Data-Tooltip="Generate"
                                                    Data-Inverted="True" Data-Variation="small">
                                                    <i class="chain icon"></i>
                                                </button>
                                            </div>
                                        </div>
            
                                        <div class="field">
                                            <label>Server Name</label>
                                            <input required type="text" name="VPS-Name" placeholder="Web-Server-1"
                                                data-validate="Data-VPS-Name">
                                        </div>
                                    </div>
            
                                    <div class="field">
                                        <label>Payload(s)</label>
                                        <select name="Payloads" multiple="" class="ui fluid search inverted selection dropdown"
                                            data-validate="Data-Payload">
                                            <option selected aria-disabled="true" aria-readonly="true" value="Payload">API
                                                (Standard)</option>
                                            <option value="Payload-NextCloud">NextCloud</option>
                                            <option value="Payload-Apache">Apache</option>
                                            <option value="Payload-Steam">Steam</option>
                                            <option value="Payload-Mordhau">Mordhau</option>
                                            <option value="Payload-Docker">Docker</option>
                                            <option value="Payload-LXD">LXD</option>
                                            <option value="Payload-ETCD">ETC-D</option>
                                            <option value="Payload-NodeJS">Node-JS</option>
                                            <option value="Payload-WordPress">WordPress</option>
